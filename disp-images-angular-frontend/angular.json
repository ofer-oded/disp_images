{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "disp-images-angular-frontend": {
      "projectType": "application",
      "schematics": {
        "@schematics/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/disp-images-angular-frontend",
      "sourceRoot": "apps/disp-images-angular-frontend/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/disp-images-angular-frontend",
            "index": "apps/disp-images-angular-frontend/src/index.html",
            "main": "apps/disp-images-angular-frontend/src/main.ts",
            "polyfills": "apps/disp-images-angular-frontend/src/polyfills.ts",
            "tsConfig": "apps/disp-images-angular-frontend/tsconfig.app.json",
            "assets": [
              "apps/disp-images-angular-frontend/src/favicon.ico",
              "apps/disp-images-angular-frontend/src/assets"
            ],
            "styles": ["apps/disp-images-angular-frontend/src/styles.scss"],
            "scripts": [],
            "vendorChunk": true,
            "extractLicenses": false,
            "buildOptimizer": false,
            "sourceMap": true,
            "optimization": false,
            "namedChunks": true
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/disp-images-angular-frontend/src/environments/environment.ts",
                  "with": "apps/disp-images-angular-frontend/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            },
            "development": {
              "buildOptimizer": false,
              "optimization": false,
              "vendorChunk": true,
              "extractLicenses": false,
              "sourceMap": true,
              "namedChunks": true
            }
          },
          "defaultConfiguration": ""
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "disp-images-angular-frontend:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "disp-images-angular-frontend:build:production"
            },
            "development": {
              "browserTarget": "disp-images-angular-frontend:build:development"
            }
          },
          "defaultConfiguration": "development"
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "disp-images-angular-frontend:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "apps/disp-images-angular-frontend/src/test.ts",
            "polyfills": "apps/disp-images-angular-frontend/src/polyfills.ts",
            "tsConfig": "apps/disp-images-angular-frontend/tsconfig.spec.json",
            "karmaConfig": "apps/disp-images-angular-frontend/karma.conf.js",
            "assets": [
              "apps/disp-images-angular-frontend/src/favicon.ico",
              "apps/disp-images-angular-frontend/src/assets"
            ],
            "styles": ["apps/disp-images-angular-frontend/src/styles.scss"],
            "scripts": []
          }
        }
      },
      "tags": []
    },
    "disp-images-angular-frontend-e2e": {
      "root": "apps/disp-images-angular-frontend-e2e",
      "sourceRoot": "apps/disp-images-angular-frontend-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/disp-images-angular-frontend-e2e/cypress.json",
            "devServerTarget": "disp-images-angular-frontend:serve:development",
            "tsConfig": "apps/disp-images-angular-frontend-e2e/tsconfig.json"
          },
          "configurations": {
            "production": {
              "devServerTarget": "disp-images-angular-frontend:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@nrwl/linter:eslint",
          "outputs": ["{options.outputFile}"],
          "options": {
            "lintFilePatterns": [
              "apps/disp-images-angular-frontend-e2e/**/*.{js,ts}"
            ]
          }
        }
      },
      "tags": [],
      "implicitDependencies": ["disp-images-angular-frontend"]
    }
  }
}
